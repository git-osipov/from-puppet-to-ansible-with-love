{
  "name": "puppet-manifests",
  "version": "1.0.0",
  "author": "openhands",
  "summary": "Collection of Puppet manifests for system configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/git-osipov/from-puppet-to-ansible-with-love",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/acl",
      "version_requirement": ">= 5.0.0 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}